 <p> This is the users show page, List result or tickets after the went on the ride</p>
<header class="header">
    #List avilable trips open to the current_user
    
          User: <%= @user.name %><br>
          Cash: <%= @user.user_cash %><br>

<br>

    <% @user.flight_rides.each do |user_flight_ride|%>
     <br>
     <p>flight_ride_id</p>
     <h3> <%= user_flight_ride.id%></h3>

     <p>user_id</p>
     <h3> <%= user_flight_ride.user_id%></h3>

     <p>flight ID Number</p>
     <h3> <%= user_flight_ride.flight_id%></h3>

<br>

    <%end%>


  <br>
      <h2><%= link_to "See all Flights", flights_path %></h2>

        <% if flash[:notice] %>
         <div class="notice"><%= flash[:notice] %></div>
        <% end %>

     <%=link_to "Log Out", logout_path, :method => :delete %>
        <%= hidden_field_tag :user_id, current_user.id %>
  <br>
   OR
  <br>

        <% if auth_hash = request.env["omniauth.auth"] %>
        <h1><%= @user.name %></h1>
        <h2>Email: <%= @user.email %></h2>
        <h2>Facebook UID: <%= @user.uid %></h2>
        <img src="<%= @user.image %>">
        <% else %>
        <%= link_to('Log in with Facebook!', '/auth/facebook') %>
        <% end %>

  </header>

  